{
  "name": "on-note",
  "productName": "on-note",
  "version": "1.0.0",
  "description": "Desktop application that controls delivery notes",
  "main": "src/index.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\"",
    "docs": "jsdoc -c jsdoc.json",
    "deploy-package": "electron-forge package --arch=ia32 --platform=win32",
    "deploy-package-linux": "electron-forge package --arch=x64 --platform=linux",
    "deploy-package-win64": "electron-forge package --arch=x64 --platform=win32",
    "deploy-make": "npm run deploy-package && node config/maker_script ia32",
    "deploy-make-64": "npm run deploy-package-win64 && node config/maker_script x64"
  },
  "keywords": [],
  "author": "virtualbox",
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "src/icons/germany.ico"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "on-note"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "electron-squirrel-startup": "^1.0.0",
    "inquirer": "^8.1.1",
    "mysql": "^2.18.1",
    "pdf-lib": "^1.16.0",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.19.0/xlsx-0.19.0.tgz"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "@types/mysql": "^2.15.17",
    "docdash": "^1.2.0",
    "electron": "11.1.0",
    "electron-reload": "^1.5.0",
    "jsdoc": "^3.6.7"
  }
}
